#include <stdio.h>
#include <stdlib.h>

struct Pair {
    int a, b;
};

int compare(const void* x, const void* y) {
    return ((struct Pair*)x)->b - ((struct Pair*)y)->b;
}

int maxChainLength(struct Pair p[], int n) {
    qsort(p, n, sizeof(struct Pair), compare);

    int count = 1;
    int last_end = p[0].b;

    for (int i = 1; i < n; i++) {
        if (p[i].a > last_end) {
            count++;
            last_end = p[i].b;
        }
    }

    return count;
}

int main() {
    struct Pair arr[] = {{5, 24}, {15, 25}, {27, 40}, {50, 60}};
    int n = sizeof(arr) / sizeof(arr[0]);

    printf("Longest Chain Length = %d\n", maxChainLength(arr, n));
    return 0;
}
